{{!
    @template qformat/h5p

    Question text template.

    Context variables required for this template:
    * questiontext - The name of the site

    Example context (json):
    {
        "questiontext": "Does Moodle do H5P?",
        "keywords": [
            {
                "options": {
                    "points": 10,
                    "occurrences":3,
                    "caseSensitive":true,
                    "forgiveMistakes":false,
                    "feedbackMissed": "To summarize the book, you should really mention the main character by name at least once.",
                 },
                 "keyword":"Bilbo",
            }
        ]
    }
}}
{{# keywords }}
    <div>{{ options.points }} {{ keyword }}: {{ options.feedbackMissed }}
{{/ keywords }}
